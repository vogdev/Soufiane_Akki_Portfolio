<%= form_for  [:admin, @portfolio] do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title  %>

  <%= f.label :description %>
  <%= f.text_field :description  %>
  
  <%= f.label :image_url %>
  <%= f.text_field :image_url  %>
  
  <%= f.label :githup_url %>
  <%= f.text_field :githup_url  %>
  
  <%= f.label :dribbble_url %>
  <%= f.text_field :dribbble_url  %>

  <%=f.submit "Update portfolio"%>

<%end%>